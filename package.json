{
  "name": "e-freshli-clone",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "cross-env API_URL=$API_URL ng build --configuration production",
    "postbuild": "cp src/assets/env.template.js dist/eFreshli-clone/browser/assets/env.js && sed -i 's|http://localhost:5000/api|'\"${API_URL}\"'|g' dist/eFreshli-clone/browser/assets/env.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
     "@abacritt/angularx-social-login": "^2.5.0",
"@angular/animations": "^20.3.1",
"@angular/cdk": "^20.3.1",
"@angular/common": "^20.3.1",
"@angular/compiler": "^20.3.1",
"@angular/core": "^20.3.1",
"@angular/forms": "^20.3.1",
"@angular/material": "^20.3.1",
"@angular/platform-browser": "^20.3.1",
"@angular/router": "^20.3.1",
"@stripe/stripe-js": "^7.9.0",
"ngx-toastr": "^19.1.0",
"rxjs": "~7.8.0",
"sweetalert": "^2.1.2",
"tslib": "^2.3.0",
"zone.js": "~0.14.3"
  },
  "devDependencies": {
 "@angular/build": "^20.3.1",
 "@angular/cli": "^20.3.1",
 "@angular/compiler-cli": "^20.3.1",
 "@types/facebook-js-sdk": "^3.3.12",
 "@types/google.accounts": "^0.0.17",
 "@types/jasmine": "~5.1.0",
 "@types/node": "^20.14.10",
 "cross-env": "^7.0.3",
 "jasmine-core": "~5.1.0",
 "karma": "~6.4.0",
 "karma-chrome-launcher": "~3.2.0",
 "karma-coverage": "~2.2.0",
 "karma-jasmine": "~5.1.0",
 "karma-jasmine-html-reporter": "~2.1.0",
"typescript": "~5.8.2"
  }
}
